{
  "name": "api-docs-cms",
  "version": "1.0.0",
  "description": "CMS for managing API documentation static site data",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
